{
  "extends": [
    "next/core-web-vitals"
  ],
  "rules": {
    "react/no-unescaped-entities": "off",
    "react/no-unknown-property": "error",
    "react/jsx-key": "error",
    "react/jsx-no-target-blank": "error",
    "no-console": ["warn", { "allow": ["warn", "error"] }],
    "no-alert": "error",
    "no-debugger": "error"
  }
} 